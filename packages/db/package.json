{
  "name": "@taiyomoe/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "seed": "prisma migrate reset && pnpm -F @taiyomoe/logs run migrate",
    "db": "prisma",
    "postinstall": "prisma generate --no-hints",
    "data-migration:add_normalized_email": "tsx ./src/prisma/migrations/20250312161343_add_normalized_email/data-migration.ts"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "cross-env PRISMA_SEED=1 tsx ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-core": "^0.12.0",
    "prisma-json-types-generator": "^3.2.2",
    "validator": "^13.12.0",
    "zod": "^3.24.2",
    "zod-prisma-types": "^3.2.4"
  },
  "devDependencies": {
    "@taiyomoe/meilisearch": "workspace:*",
    "@taiyomoe/tsconfig": "workspace:*",
    "@types/validator": "^13.12.2",
    "cross-env": "^7.0.3",
    "prisma": "^6.5.0",
    "tsx": "^4.19.3"
  }
}
