{
  "name": "@taiyomoe/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "seed": "prisma migrate reset && pnpm -F @taiyomoe/logs run migrate",
    "db": "prisma",
    "migrate": "tsx ./src/migrations-handler.ts",
    "postinstall": "prisma generate",
    "data-migration:add_normalized_email": "tsx ./src/prisma/migrations/20250312161343_add_normalized_email/data-migration.ts",
    "data-migration:add_usernames": "tsx ./src/prisma/migrations/20250312171952_add_usernames/data-migration.ts"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "cross-env PRISMA_SEED=1 tsx ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@prisma/client": "^6.8.2",
    "@t3-oss/env-core": "^0.13.4",
    "@taiyomoe/utils": "workspace:^",
    "prisma-json-types-generator": "^3.4.2",
    "validator": "^13.15.0",
    "zod": "^3.25.30",
    "zod-prisma-types": "^3.2.4"
  },
  "devDependencies": {
    "@taiyomoe/meilisearch": "workspace:^",
    "@taiyomoe/tsconfig": "workspace:^",
    "@types/validator": "^13.15.1",
    "cross-env": "^7.0.3",
    "prisma": "^6.8.2",
    "tsx": "^4.19.4"
  }
}
