{
  "name": "@taiyomoe/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./env.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "seed": "prisma db push --force-reset && prisma generate && prisma db seed",
    "db": "prisma",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "cross-env PRISMA_SEED=1 tsx ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.10.1",
    "@prisma/client": "^5.17.0",
    "prisma-json-types-generator": "^3.0.4",
    "zod-prisma-types": "^3.1.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@taiyomoe/meilisearch": "workspace:*",
    "@taiyomoe/tsconfig": "workspace:*",
    "cross-env": "^7.0.3",
    "prisma": "^5.17.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
