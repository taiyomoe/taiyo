{
  "name": "@taiyomoe/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "seed": "pnpm with-env prisma db push --force-reset && prisma generate && prisma db seed",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "cross-env PRISMA_SEED=1 pnpm with-env tsx ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0"
  },
  "devDependencies": {
    "@taiyomoe/meilisearch": "workspace:^0.1.0",
    "@taiyomoe/tsconfig": "workspace:^0.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "prisma": "^5.13.0",
    "prisma-json-types-generator": "^3.0.4",
    "tsx": "^4.8.1",
    "typescript": "^5.4.5",
    "zod-prisma-types": "^3.1.6"
  }
}
