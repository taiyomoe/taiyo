{
  "name": "@taiyomoe/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "seed": "prisma migrate reset && pnpm -F @taiyomoe/logs run migrate",
    "db": "prisma",
    "postinstall": "prisma generate --no-hints"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "cross-env PRISMA_SEED=1 tsx ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@prisma/client": "^6.0.1",
    "@t3-oss/env-core": "^0.11.1",
    "prisma-json-types-generator": "^3.2.2",
    "zod": "^3.24.0",
    "zod-prisma-types": "^3.2.1"
  },
  "devDependencies": {
    "@taiyomoe/meilisearch": "workspace:*",
    "@taiyomoe/tsconfig": "workspace:*",
    "cross-env": "^7.0.3",
    "prisma": "^6.0.1",
    "tsx": "^4.19.2"
  }
}
