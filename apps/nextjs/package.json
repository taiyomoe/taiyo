{
  "name": "@taiyo/nextjs",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path=../../tooling/prettier/.prettierignore",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@taiyo/api": "^0.1.0",
    "@taiyo/auth": "^0.1.0",
    "@taiyo/db": "^0.1.0",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@trpc/client": "^10.38.3",
    "@trpc/next": "^10.38.3",
    "@trpc/react-query": "^10.38.3",
    "@trpc/server": "^10.38.3",
    "@wits/next-themes": "^0.2.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.277.0",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "swiper": "^10.2.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@taiyo/eslint-config": "^0.2.0",
    "@taiyo/prettier-config": "^0.1.0",
    "@taiyo/tailwind-config": "^0.1.0",
    "@taiyo/tsconfig": "^0.1.0",
    "@types/node": "^18.17.15",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@taiyo/eslint-config/base",
      "@taiyo/eslint-config/nextjs",
      "@taiyo/eslint-config/react"
    ]
  },
  "prettier": "@taiyo/prettier-config"
}
